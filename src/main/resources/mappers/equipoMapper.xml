<mapper namespace="com.mycompany.mappers.EquipoMapper">
     <resultMap type='Equipo' id='EquipoResult'>
        <id property='placa' column='Placa'/>  
             <result property='nombre' column='nombre_Equipo'/>      
        <result property='imagen' column='imagen'/>
        <result property='marca' column='marca'/>
        <result property='vidaUtil' column='vida_util'/>
         <result property='precio' column='Precio'/>
         <result property='observaciones' column='Observaciones'/>
       <!-- <association property='suscriptor' resultMap='SuscriptorResult' columnPrefix='sus_'></association>-->
    </resultMap> 
</mapper>
<insert id="insertEquipo" parameterType="map">
    INSERT INTO Equipo (Placa,Observaciones,Seria,Estado,Precio,Tipo_Equipo_Modelo,prestamo_solicitud_id) values(#{equipo.placa},#{equipo.observaciones},#{equipo.serial},#{equipo.estado},#{equipo.precio},#{fk_tipo_modelo},#{fk_pres_sctud})   
</insert>
<select id="loadeqByid" resultMap="EquipoResult">
    SELECT * FROM Equipo eq WHERE eq.Placa = #{plac}
</select>
    